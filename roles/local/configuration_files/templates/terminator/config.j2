[global_config]
  scroll_tabbar = True
  title_font = Fira Code Light 9
  title_use_system_font = False
[keybindings]
  close_term = <Primary>w
  close_window = <Primary>q
  copy = <Primary>c
  full_screen = <Alt>Return
  new_tab = <Primary>t
  new_window = <Primary>n
  paste = <Primary>v
  search = <Primary>f
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#300a24"
    background_darkness = 0.76
    cursor_color = "#aaaaaa"
    font = Fira Code Light 10
    foreground_color = "#ffffff"
    scrollback_infinite = True
    use_system_font = False
